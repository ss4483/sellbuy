<li>
  <div class="row"  style="margin-bottom:0; width:210px; height:55px;">
    <a class="modal-trigger_ep left row col s10" href="#ep_<%=post.id%>" style="margin-bottom:0; height: 55px;padding:0px; display: inline-block;">
      <div class="col s4 valign-demo valign-wrapper" style=" padding:5px;">
        <% unless post.image_url.nil? %>
          <img class="valign center flea_img_s" src="<%= post.image_url %>" alt="your image"  style="display: block; margin-left: auto; margin-right: auto; max-height:100%; width:auto; max-width:100%; height:auto; vertical-align: middle"/>
        <% end %>
      </div>
      <div class="col s8" style="height: 55px; line-height: 30px; padding-top: 7px;">
      <span class="left" style="font-size:10px; color:black;"><%= user_nickname_ep(post.user.id) %>   </span>
      <span class="star-rating small_star left" style="width:75px; height:15px;">
          <% 1.upto(5) do |i| %>
            <% if i == post.rating.to_i %>
              <i class="on"></i>
            <% else %>
              <i></i>
            <% end %>
          <% end %>
        </span>
      </div>
    </a>

    <div class="col s2" style="padding:0px; height:55px;">
      <% if user_signed_in? && post.user == current_user  %>
        <a class="modal-trigger enjoy-css right" href="#update<%= post.id%>" style="position:relative; padding:0px; width:100%; height:27.5px;">
          <i class="material-icons" style="font-size:1.2rem;height:27.5px; position:absolute; left:10px; top:-18px;">mode_edit</i></a>
        <a class="enjoy-css right modal-trigger" href="#post_delete_modal_<%= post.id %>" style="position:relative; padding:0px; width:100%; height:27.5px;">
          <i class="material-icons" style="font-size:1.2rem;height:27.5px; position:absolute; left:10px; top:-18px;">delete</i></a>

      <% elsif user_signed_in? && @one_user == current_user && post.index_check %>
        <a class="enjoy-css" href="/epilogue_check/<%=post.id%>" style="width:100%; padding:0px; height:55px;">
          <i class="tiny material-icons" style="width:100%; padding:0px; height:55px;">settings_backup_restore</i>
        </a>
      <% elsif user_signed_in? && @one_user == current_user  %>
        <a class="enjoy-css" href="/epilogue_check/<%=post.id%>"style="width:100%; padding:0px; height:55px;">
          <i class="small material-icons" style="width:100%; padding:0px; height:55px;">done</i>
        </a>
      <% end %>
    </div>
  </div>
</li>
